             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:vw="clr-namespace:Calendar_Converter.View"
             mc:Ignorable="d" Height="100" Width="560">
    <Grid>
        <Border BorderBrush="Black" Margin="2">
        <UniformGrid Columns="7" Rows="1">
            <vw:Day/>
            <vw:Day/>
            <vw:Day/>
            <vw:Day/>
            <vw:Day/>
            <vw:Day/>
