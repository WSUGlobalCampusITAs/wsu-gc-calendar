        	<TextBlock TextWrapping="Wrap" Text="Sunday" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="6" Grid.Row="2"/>
        	<TextBlock x:Name="tblkOldMonday" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkOldTuesday" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkOldWednesday" Grid.Row="1" Grid.Column="2" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkOldThursday" Grid.Row="1" Grid.Column="3" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkOldFriday" Grid.Row="1" Grid.Column="4" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkOldSaturday" Grid.Row="1" Grid.Column="5" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkOldSunday" Grid.Row="1" Grid.Column="6" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkNewMonday" Grid.Row="3" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkNewTuesday" Grid.Row="3" Grid.Column="1" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkNewWednesday" Grid.Row="3" Grid.Column="2" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkNewThursday" Grid.Row="3" Grid.Column="3" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkNewFriday" Grid.Row="3" Grid.Column="4" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkNewSaturday" Grid.Row="3" Grid.Column="5" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        	<TextBlock x:Name="tblkNewSunday" Grid.Row="3" Grid.Column="6" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        </Grid>
        <Label Content="Calendar Converter" HorizontalAlignment="Left" Height="40" Margin="10,12.053,0,0" VerticalAlignment="Top" Width="257" FontFamily="Times New Roman" FontSize="24" Foreground="#FFB8B8D3"/>

    </Grid>
</Window>
