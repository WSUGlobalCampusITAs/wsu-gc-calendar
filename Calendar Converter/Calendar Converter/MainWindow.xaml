        	<Rectangle Grid.Column="3" Fill="#FF8D3106" Grid.Row="2" Stroke="Black"/>
        	<Rectangle Grid.Column="3" Fill="#FFBD7964" Grid.Row="3" Stroke="Black"/>
        	<Rectangle Grid.Column="4" Fill="#FF555595" Stroke="Black"/>
        	<Rectangle Grid.Column="4" Fill="#FF8D3106" Grid.Row="2" Stroke="Black"/>
        	<Rectangle Grid.Column="4" Fill="#FFBD7964" Grid.Row="3" Stroke="Black"/>
        	<Rectangle Grid.Column="5" Fill="#FF555595" Stroke="Black"/>
        	<Rectangle Grid.Column="5" Fill="#FFB8B8D3" Grid.Row="1" Stroke="Black" />
        	<Rectangle Grid.Column="5" Fill="#FF8D3106" Grid.Row="2" Stroke="Black"/>
        	<Rectangle Grid.Column="5" Fill="#FFBD7964" Grid.Row="3" Stroke="Black"/>
        	<Rectangle Grid.Column="6" Fill="#FF555595" Stroke="Black"/>
        	<Rectangle Grid.Column="6" Fill="#FFB8B8D3" Grid.Row="1" Stroke="Black"/>
        	<Rectangle Grid.Column="6" Fill="#FF8D3106" Grid.Row="2" Stroke="Black"/>
        	<Rectangle Grid.Column="6" Fill="#FFBD7964" Grid.Row="3" Stroke="Black"/>
        	<Rectangle Grid.Column="1" Fill="#FFBD7964" Grid.Row="3" Stroke="Black"/>
        	<Rectangle Grid.Column="1" Fill="#FF8D3106" Grid.Row="2" Stroke="Black"/>
        	<Rectangle Grid.Column="1" Fill="#FFB8B8D3" Grid.Row="1" Stroke="Black"/>
        	<Rectangle Grid.Column="1" Fill="#FF555595" Stroke="Black" />
        	<Rectangle Fill="#FFBD7964" Grid.Row="3" Stroke="Black"/>
        	<Rectangle Fill="#FF8D3106" Grid.Row="2" Stroke="Black"/>
        	<Rectangle Fill="#FFB8B8D3" Grid.Row="1" Stroke="Black"/>
        	<Rectangle Fill="#FF555595"  Stroke="Black" />
        	<TextBlock TextWrapping="Wrap" Text="Monday" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        	<TextBlock TextWrapping="Wrap" Text="Tuesday" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="1"/>
        	<TextBlock TextWrapping="Wrap" Text="Monday" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="2"/>
        	<TextBlock TextWrapping="Wrap" Text="Wednesday" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="2"/>
        	<TextBlock TextWrapping="Wrap" Text="Tuesday" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="2" Grid.Column="1"/>
        	<TextBlock TextWrapping="Wrap" Text="Thursday" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="3"/>
        	<TextBlock TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="2" Grid.Column="2" Text="Wednesday"/>
        	<TextBlock TextWrapping="Wrap" Text="Friday" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="4"/>
