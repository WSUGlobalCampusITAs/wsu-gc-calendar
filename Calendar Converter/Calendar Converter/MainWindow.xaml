<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="Calendar_Converter.MainWindow"
        Title="MainWindow" Height="540" Width="816">
	<Grid Margin="0,0,2,1" Background="#FF5B5A74">
		<DatePicker x:Name="dtpickOldStart" HorizontalAlignment="Right" Height="28" Margin="0,22,10,0" VerticalAlignment="Top" Width="156" Background="Transparent"/>
		<DatePicker x:Name="dtpickNewStart" HorizontalAlignment="Right" Height="31" Margin="0,66,10,0" VerticalAlignment="Top" Width="156"/>
		<Label Content="Old Start Date" HorizontalAlignment="Right" Height="28" Margin="0,22,216,0" VerticalAlignment="Top" Width="125"/>
		<Label Content="New Start Date" HorizontalAlignment="Right" Height="31" Margin="0,66,216,0" VerticalAlignment="Top" Width="125"/>
		<TextBox x:Name="txtbxNumWeeks" HorizontalAlignment="Right" Height="19" Margin="0,111,10,0" VerticalAlignment="Top" Width="26" TextAlignment="Center"/>
		<Label Content="Number of Weeks" HorizontalAlignment="Right" Height="23" Margin="0,107,216,0" VerticalAlignment="Top" Width="125"/>
		<Button x:Name="BtnUpdate" Content="Update" HorizontalAlignment="Right" Height="26" Margin="0,162,10,0" VerticalAlignment="Top" Width="156" Background="#FFBD7964"/>
        <RadioButton  x:Name="rbSkipbreak" Content="Skip Breaks" HorizontalAlignment="Right" Height="19" Margin="0,141,211,0" VerticalAlignment="Top" Width="130" Checked="RadioButton_Checked" GroupName="Breaks"/>
        <RadioButton  x:Name="rbNoskip" Content="Do Not Skip Breaks" HorizontalAlignment="Right" Height="19" Margin="0,160,211,0" VerticalAlignment="Top" Width="130" Checked="RadioButton_Checked" GroupName="Breaks" IsChecked="True"/>
        <Button x:Name="btnNext" Content="Next" Margin="0,0,10,9" Height="26" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="73"/>
        <Button x:Name="btnPrevious" Content="Previous" Margin="0,0,88,9" HorizontalAlignment="Right" Width="78" Height="26" VerticalAlignment="Bottom"/>
        <Grid Margin="10,229,10,42">
        	<Grid.RowDefinitions>
        		<RowDefinition Height="26*"/>
        		<RowDefinition Height="93*"/>
        		<RowDefinition Height="26*"/>
        		<RowDefinition Height="93*"/>
        	</Grid.RowDefinitions>
        	<Grid.ColumnDefinitions>
        		<ColumnDefinition Width="83*"/>
        		<ColumnDefinition Width="83*"/>
        		<ColumnDefinition Width="83*"/>
        		<ColumnDefinition Width="83*"/>
        		<ColumnDefinition Width="83*"/>
        		<ColumnDefinition Width="83*"/>
        		<ColumnDefinition Width="83*"/>
        	</Grid.ColumnDefinitions>
        	<Rectangle Grid.Column="4" Fill="#FFB8B8D3" Grid.Row="1" Stroke="Black" />
        	<Rectangle Grid.Column="2" Fill="#FF555595" Stroke="Black" />
        	<Rectangle Grid.Column="2" Fill="#FFB8B8D3" Grid.Row="1" Stroke="Black" />
        	<Rectangle Grid.Column="2" Fill="#FF8D3106" Grid.Row="2" Stroke="Black"/>
