        	<TextBlock TextWrapping="Wrap" Text="Saturday" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="5"/>
        	<TextBlock TextWrapping="Wrap" Text="Friday" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="2" Grid.Column="4"/>
        	<TextBlock TextWrapping="Wrap" Text="Sunday" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="6"/>
